            */
            addScript: (src, tag, content) => {
                const script = document.createElement('script');
                script.type = 'text/javascript';
                if (src) {
                    script.src = src;
                }
                if (content) {
                    script.text = content;
                }
                script.async = true;
                const firstScript = tag != null ? document.querySelector(`script[src="${tag}"]`) : document.getElementsByTagName('script')[0];
